# ################################################################ Build platform/common# ###############################################################.PHONY: platform_commonplatform_common:	@$(MAKE) -C $(TOPDIR)platform/common/driver	@$(MKDIR) -p $(KERNEL_OUT_MODULES)/lib/modules/common	@$(CP) -f $(OUT_DIR)/platform/common/driver/$(SYSCONFIG_PANEL).ko $(KERNEL_OUT_MODULES)/lib/modules/common/.PHONY: platform_common_cleanplatform_common_clean:	@$(MAKE) -C $(TOPDIR)platform/common/driver clean	@$(RM) -rf $(KERNEL_OUT_MODULES)/lib/modules/common